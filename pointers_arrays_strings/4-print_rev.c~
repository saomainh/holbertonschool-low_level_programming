#include <stdlib.h>
#include <stdio.h>
#include "main.h"

/**
 * print_rev - Short description, single line
 * @str: aerfae
 * Description: this is a description
 * Return: Description of the returned value
 */

void print_rev(char *str)
{
	int count, i;
	
	while (*str != '\0')
	{
		count++;
		str++;
	}
	str -= count;
	for (i = count; i >= 0; i--)
	{
		_putchar(str[i]);
	}		
	_putchar ('\n');
}
