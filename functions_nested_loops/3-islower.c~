#include <stdlib.h>
#include <stdio.h>
#include "main.h"


/**
 * print_alphabet_x10 - Short description, single line
 * Description: this is a description
 * Return: Description of the returned value
 */

int _islower(int c)
{
	if (c >= 97 && c <= 122)
	{
		return (1);
	}
	else
	{
		return (0);
	}
}
